{
  "name": "google-calendar-mcp-server",
  "version": "1.0.0",
  "description": "Google Calendar MCP Server with OAuth2 authentication",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run build:deps",
    "build:deps": "npm ci --only=production",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "auth": "tsx src/auth.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "googleapis": "^144.0.0",
    "@google-cloud/local-auth": "^3.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "tsx": "^4.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
